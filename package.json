{
  "name": "c-struct-js",
  "version": "0.0.1",
  "description": "UMD module for DataView extension inspired by C structs",
  "main": "umd/struct.min.js",
  "scripts": {
    "build": "gulp",
    "git": "git push origin master",
    "prepublishOnly": "npm run build && npm test && npm run git",
    "test": "standard"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/patrickroberts/c-struct-js.git"
  },
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=4.0.0"
  },
  "author": "Patrick Roberts",
  "license": "MIT",
  "standard": {
    "parser": "babel-eslint"
  },
  "transform": [
    [
      "babelify",
      {
        "comments": false
      }
    ]
  ],
  "devDependencies": {
    "babel-eslint": "^8.0.0",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-new-target": "^7.0.0-alpha.14",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "gulp": "^3.9.1",
    "gulp-sourcemaps": "^2.6.1",
    "gulp-uglify": "^3.0.0",
    "gulp-util": "^3.0.8",
    "jsdoc-babel": "^0.3.0",
    "jsdoc-to-markdown": "^3.0.0",
    "require-dir": "^0.3.2",
    "standard": "^10.0.3",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "dependencies": {
    "buffer": "^5.0.7"
  }
}
